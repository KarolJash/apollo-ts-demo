{
  "name": "backend-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate code for any backend project with typescript.",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon",
    "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
    "build": "tsc -p tsconfig.prod.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:migration": "npx prisma migrate dev",
    "generate:reset": "npx prisma migrate reset --force",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "Karol Jasniewicz",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/client-preset": "^1.2.4",
    "@graphql-codegen/typescript": "^2.8.6",
    "@graphql-codegen/typescript-resolvers": "^2.7.11",
    "@types/dotenv-safe": "^8.1.2",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "*"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@graphql-codegen/core": "^2.6.8",
    "@graphql-codegen/plugin-helpers": "^3.1.2",
    "@graphql-codegen/schema-ast": "^2.6.1",
    "@graphql-tools/schema": "^9.0.12",
    "@prisma/client": "^4.8.0",
    "apollo-graphql": "^0.9.7",
    "apollo-server": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.6.0",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-server-express": "^1.4.1",
    "graphql-tools": "^8.3.14",
    "http": "^0.0.1-security"
  }
}
